{
  "name": "autograd",
  "version": "1.0.0",
  "description": "Under Process",
  "main": "index.js",
  "scripts": {
    "installdependenciespython": "pip3 install -r requirements.txt --prefix flask-server",
    "installnpm": "npm install --prefix client",
    "dev": "concurrently \"npm run installdependenciespython\" \"npm run installnpm\"",
    "server": "python3 app.py --prefix flask-server",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
